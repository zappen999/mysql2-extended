{
	"compilerOptions": {
		"noEmit": true,
		"baseUrl": "./src",
		"lib": ["es6"],
		"importsNotUsedAsValues": "error",
		"noUncheckedIndexedAccess": true,
		"strict": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
		"typeRoots": [
			"./node_modules/@types"
		]
	},
	"include": [
		"./src/"
	]
}
